/* //Revamped Top-Teasers */
#top-teasers .teaser {
  margin: 0;
  border: 0;
  padding: 0;
  overflow: visible;
}

#top-teasers .item-0 {
  position: absolute;
  left: 18px;
  top: 7px;
  /* width: 128px; */
}

.stock-teaser A {
  color: #000;
}

.stock-teaser .stock-item {
  margin: 6px 0 0 0;
  border-top: 1px dotted #E2E2E2;
  padding: 6px 0 0 0;
}

.stock-teaser .stock-item:first-child {
  margin-top: 0;
  border-top: 0;
  padding-top: 0;
}

.stock-teaser H2, .stock-teaser .title {
  margin: 0 0 0 4px;
  font-size: 16px;
  font-weight: bold;
}

.stock-teaser .date {
  font-size: 10px;
  font-weight: normal;
  color: #636363;
}

.stock-teaser P {
  height: 32px;
  margin: 0;
  /*padding: 0 0 0 35px;*/
  background: url(../images/icon_stock.png) no-repeat 0 -168px;
}

.stock-teaser A:hover,.stock-teaser A:focus {
  text-decoration: none;
}

.stock-teaser .change {
  display: block;
  font-weight: bold;
  font-size: 15px;
  padding: 0 0 0 35px;
}

.stock-teaser .stock-up P {
  background-position: 0 0;
}

.stock-teaser .stock-up .change {
  color: #339900;
}

.stock-teaser .stock-down P {
  background-position: 0 -84px;
}

.stock-teaser .stock-down .change {
  color: #f00808;
}

.stock-teaser .total {
  display: block;
  font-size: 10px;
  font-weight: normal;
  color: #636363;
  padding: 0 0 0 35px;
}

