#menus {
  position: absolute;
  left: 0;
  top: 141px;
  z-index: 998;
  font-size: 13px;
  line-height: 1.0;
}

#menus UL {
  margin: 0;
  padding: 0;
}

#menus LI {
  float: left;
  margin: 0;
  list-style: none;
}

#menus A {
  float: left;
}

#menu {
  border-bottom: 1px solid #e2e2e2;
  display: block;
  height: 30px;
  list-style-type: none;
  margin: 10px 0;
  overflow: visible;
  padding: 0;
  /* Kolla IE om nedan behövs*/
  width: 964px;
}

#menu li {
  display: inline;
  float: left;
  margin: 0;
  padding: 0;
  width: auto;
}

#menu li a {
  background-color: #f9f9f9;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#ffffff),
    to(#f9f9f9) );
  background-image: -webkit-linear-gradient(top, #ffffff, #f9f9f9);
  background-image: -moz-linear-gradient(top, #ffffff, #f9f9f9);
  background-image: -o-linear-gradient(top, #ffffff, #f9f9f9);
  background-image: linear-gradient(to bottom, #ffffff, #f9f9f9);
  border: 1px solid #e2e2e2;
  color: #333;
  display: block;
  font-size: 14px;
  font-weight: bold;
  height: 13px;
  margin: 3px 2px 0 0;
  padding: 5px 11px;
  text-decoration: none;
}

#menu li a:focus,#menu li a:hover {
  background-color: #f0f0f0;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#f8f8f8),
    to(#f0f0f0) );
  background-image: -webkit-linear-gradient(top, #f8f8f8, #f0f0f0);
  background-image: -moz-linear-gradient(top, #f8f8f8, #f0f0f0);
  background-image: -o-linear-gradient(top, #f8f8f8, #f0f0f0);
  background-image: linear-gradient(to bottom, #ffffff, #f0f0f0);
}

#menu li a:active {
  background: none repeat scroll 0 0 #fff;
}

#menu li.selected a {
  background: none repeat scroll 0 0 #fff;
  border-bottom: 0 none;
  color: #111111;
  height: 17px;
  margin-top: 0;
  padding: 6px 25px;
  /*padding-top: 7px;*/
  border-top: 2px solid #333;
}

#menu li>div {
  display: none;
  position: absolute;
  top: 30px;
  width: 917px;
  left: 0;
  z-index: 999;
  background: #FFF;
  -webkit-box-shadow: 0px 9px 15px rgba(0, 0, 0, 0.5);
  box-shadow: 0px 9px 15px rgba(0, 0, 0, 0.5);
  padding: 10px 37px 15px 10px;
}

#menu .active>div {
  display: block;
}

#menu li>div ul {
  float: left;
  margin-left: 20px;
  width: 214px;
}

#menu li>div ul:first-child {
  margin-left: 0;
}

#menu li>div li {
  display: list-item;
  float: none;
}

#menu li>div li a {
  display: inline-block;
  *display: inline;
  *zoom: 1;
  float: none;
  background: transparent;
  color: #0084FF;
  font-size: 12px;
  border: 0;
  margin: 3px 0 0 0;
  padding: 0;
}

#menu li>div li a:focus,#menu li>div li a:hover {
  background: transparent;
  color: #333;
}

#menu .index-title {
  display: block;
  font-weight: bold;
  margin-top: 12px;
}

#menu li:first-child .index-title {
  margin-top: 0;
}

#menu .target-close {
  width: 17px;
  height: 17px;
  display: block;
  position: absolute;
  top: 0;
  right: 0;
  margin: 0;
  text-indent: -9999px;
  overflow: hidden;
  background: url("../images/icon_close.png") no-repeat 10px 10px;
  border: 0;
  padding: 10px;
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=85)";
  filter: alpha(opacity =   85);
  -khtml-opacity: 0.85;
  -moz-opacity: 0.85;
  opacity: 0.85;
}

#menu .target-close:focus,#menu .target-close:hover {
  background: url("../images/icon_close.png") no-repeat 10px 10px;
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
  filter: alpha(opacity =   100);
  -khtml-opacity: 1;
  -moz-opacity: 1;
  opacity: 1;
}

#submenu { /*background: #494949;*/
  float: left;
  width: 964px;
}

#submenu A {
  padding: 5px 6px 7px 6px;
  color: #333;
  /*text-shadow: 0 1px #000000;*/
}

#submenu A:hover,#submenu A:focus {
  text-decoration: none;
  color: #000;
}

#submenu .selected {
  font-weight: bold;
  text-decoration: underline;
}

#submenu LI {
  position: relative;
}

#submenu LI A {
  padding: 4px 10px 5px 10px;
}

#submenu LI.has-submenu A,#submenu LI.has-submenu LI.has-submenu A {
  background-image: url(../images/icon_submenu.png);
  background-repeat: no-repeat;
  background-position: 100% 9px;
}

#submenu LI.has-submenu LI A,#submenu LI.has-submenu LI.has-submenu LI A
  {
  background-image: none;
}

#submenu LI UL {
  display: none;
  position: absolute;
  left: 0;
  top: 22px;
  background: none repeat scroll 0 0 #ffffff;
  padding: 5px;
  border: 1px solid #d6d6d6;
  box-shadow: 0 9px 15px rgba(0, 0, 0, 0.5);
}

#submenu LI LI {
  float: none;
  margin: 0;
  background: #fff;
  /*border-top: 1px dotted #333;*/
  z-index: 1000;
}

#submenu LI LI:first-child { /*border-top-style: solid;*/

}

#submenu LI LI A {
  display: block;
  width: 105px;
  float: none;
  padding-top: 6px;
  padding-bottom: 6px;
}

#submenu LI LI A:hover,#submenu LI LI A:focus {
  background: #f1f1f1;
  color: #000;
}

#submenu LI.has-submenu LI.has-submenu A {
  width: 100px;
  padding-right: 15px;
  background-position: 112px 10px;
}

#submenu LI:hover UL {
  display: block;
}

#submenu LI:hover UL UL {
  left: 170px;
  top: 0;
  display: none;
  padding-left: 5px;
}

#submenu LI UL LI:hover UL {
  display: block;
  background-position: 0 0;
}

#submenu LI UL LI LI:first-child {
  border-top: none;
}

