.page-prenumerera.page-login #content .article .buttonMore {
  position: absolute;
  bottom: 30px;
  left: 176px;
  z-index: 9020;
  color: white;
}

.page-prenumerera.page-login #content .article .buttonMore span {
  text-transform: none;
