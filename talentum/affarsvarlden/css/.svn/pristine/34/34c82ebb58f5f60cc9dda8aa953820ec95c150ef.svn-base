/** Changed mar 25 **/
html {
  color: #000;
  background: #fff;
}

body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td
  {
  margin: 0;
  padding: 0;
}

table {
  border-collapse: collapse;
  border-spacing: 0;
}

fieldset,img {
  border: 0;
}

address,caption,cite,code,dfn,em,strong,th,var {
  font-style: normal;
  font-weight: normal;
}

li {
  list-style: none;
}

caption,th {
  text-align: left;
}

h1,h2,h3,h4,h5,h6 {
  font-size: 100%;
  font-weight: normal;
}

q:before,q:after {
  content: '';
}

abbr,acronym {
  border: 0;
  font-variant: normal;
}

sup {
  vertical-align: text-top;
}

sub {
  vertical-align: text-bottom;
}

input,textarea,select {
  font-family: inherit;
  font-size: inherit;
  font-weight: inherit;
}

legend {
  color: #000;
}

/* phone/pad Message*/
.phoneMessage,.padMessage {
  width: 100%;
  text-align: center;
  /* Margin-fix for responsive margin:26px 0 -26px 0; */
  padding: 10px 0;
  border-bottom: 1px solid #ccc;
  background-color: #ffffff;
}

.phoneMessage a {
  font-weight: normal;
  text-decoration: none !important;
  font-family: arial;
  color: #000; ?
  font-size: 3em;
}

.padMessage a {
  font-weight: normal;
  text-decoration: none !important;
  font-family: arial;
  color: #000;
  font-size: 1.6em;
}

/*
    * Oktavilla Baseline CSS
    * Version 1.0
    */
BODY {
  font: 100%/1.0 sans-serif;
  -webkit-text-size-adjust: none;
}

OL {
  padding-left: 2em;
}

OL LI {
  list-style: decimal outside;
}

UL {
  padding-left: 1.5em;
}

UL LI {
  list-style: square outside;
}

B,STRONG {
  font-weight: bold;
}

EM,I {
  font-style: italic;
}

B EM,B I,I B,EM B,STRONG EM,STRONG I,I STRONG,EM STRONG {
  font-weight: bold;
  font-style: italic;
}

TH {
  font-weight: bold;
}

TD,TH {
  padding: 2px;
}

P,TABLE,UL,OL {
  margin-bottom: .7em;
}

INPUT,TEXTAREA,SELECT {
  padding: 4px;
  border: 1px solid #ccc;
}

.checkbox INPUT,INPUT.checkbox,.radio INPUT,INPUT.radio,INPUT[type=checkbox]
  {
  border: none;
  padding: 0;
  width: auto;
}

INPUT.submit {
  width: auto !important;
  background: #353535 url(../images/bg_button.png) repeat-x top left;
  border: 0;
  color: white;
  padding: 3px;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  cursor: pointer;
}

INPUT.submit:hover {
  background-color: #353535;
  background-position: 0 -7px;
}

/* SCAFFOLDING */
#page {
  background: white;
  width: 964px;
  margin: 0;
  padding: 50px 20px 20px 20px;
  font: 0.75em/1.2 arial, verdana, sans-serif;
  color: black;
  position: relative;
}

#header {
  min-height: 198px;
  margin: 0 0 12px 0;
  z-index: 1;
}

#content {
  overflow: hidden;
  margin: 0;
}

#footer {
  position: relative;
  padding: 46px 0 0 0;
}

/* SIX */
#sixBody {
  font: 0.75em/1.2 arial, verdana, sans-serif;
  background: #fff;
}

#sixBody .tabbed-box {
  margin: 0px;
}

