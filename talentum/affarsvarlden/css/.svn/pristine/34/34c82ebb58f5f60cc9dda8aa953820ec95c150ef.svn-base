/* GENERAL */
HTML {
  background: #F9F9F9;
}

BODY.font-size-large .article P,BODY.font-size-large .article UL,BODY.font-size-large .article OL,BODY.font-size-large .article H3
  {
  font-size: 1.2em;
}

BODY.font-size-large .article P.lead {
  font-size: 1.5em;
}

A {
  color: #0C96ED;
  text-decoration: none;
}

A:hover {
  text-decoration: underline;
}

.skip {
  position: absolute;
  left: -9999px;
  top: -9999px;
}

.date {
  color: #999;
  white-space: nowrap;
}

.comments {
  white-space: nowrap;
}

.columns-1-1 .first,.columns-1-2 .first,.columns-3-1 .first,.columns-1-1-1 .first
  {
  border-top: 0 !important;
  margin-top: 0 !important;
  padding-top: 0 !important;
}

.more {
  float: right;
  margin: 5px 0 0 0;
  color: #666;
  line-height: 1.15;
  min-height: 10px;
}

A.more:hover {
  cursor: pointer;
}

A.rss {
  background: url(../images/icon_rss.gif) no-repeat 100% 0px;
  padding-right: 17px;
}

.has-error {
  background: #fff9b5;
}

.alert-box {
  background: #fff9b5;
  padding: 10px;
  margin: 0 0 20px 0;
}

.alert-box-heading {
  font-weight: bold;
  font-size: 16px;
}

.alert-box .alert {
  color: red;
  font-weight: bold;
}

.alert-box LI {
  margin: 0 0 2px 1.2em;
  list-style-type: square;
}

.alert-box A {
  display: inline;
  padding: 0;
  margin: 0;
  color: black;
}

.display-block {
  display: block !important;
}

.display-none {
  display: none !important;
}

H1.hidden {
  position: absolute;
  top: -9999px;
}

TABLE {
  width: 100%;
}

#page-cover {
  background: #666;
  opacity: 0.5;
}

/* DEFAULT FORM DESIGN */
.form-box {
  padding: 15px 0;
}

.form-box LABEL {
  display: block;
  font-weight: bold;
}

.form-box LABEL SPAN {
  font-weight: normal;
  padding: 0 0 0 1px;
  color: #6699cc;
}

.form-box H2 {
  font-weight: bold;
  margin: 0 0 2px 0;
}

.form-box .radio-group LABEL,.form-box .radio-group INPUT {
  display: inline;
  font-weight: normal;
}

.form-box .radio-group INPUT {
  width: auto !important;
}

.form-box .radio-group {
  overflow: hidden;
}

.form-box .radio-group DIV {
  overflow: hidden;
}

.form-box .radio-group INPUT {
  border: 0 !important;
  margin: 0 !important;
}

/* ADVANCED SEARCH FORM */
.form-box-advanced-search {
  margin: -15px 0 10px 0;
}

.form-box-advanced-search .free-text LABEL {
  display: none;
}

.form-box-advanced-search .free-text INPUT {
  width: 300px;
  margin: 0 5px 0 0;
  float: left;
  padding: 4px;
}

.form-box-advanced-search .free-text {
  overflow: hidden;
}

.form-box-advanced-search .time-frame {
  margin: 10px 0 0 0;
  overflow: hidden;
  position: relative;
}

.form-box-advanced-search A.toggle {
  position: absolute;
  bottom: 0;
  left: 0;
}

.form-box-advanced-search DIV {
  overflow: hidden;
}

.form-box-advanced-search .time-frame DIV {
  margin: 0 6px 20px 0;
  float: left;
}

.form-box-advanced-search .time-frame LABEL {
  display: inline;
  font-weight: normal;
}

.form-box-advanced-search .time-frame INPUT {
  border: none;
  margin: 0 3px 0 0;
  width: auto;
}

.form-box-advanced-search #help {
  padding: 6px 0 0 0;
  margin: 6px 0 0 0;
  border-top: 1px solid #ccc;
  display: none;
  color: #666;
  clear: both;
}

/* LOADER */
.loader {
  text-align: center;
  font-size: 20px;
  padding: 80px 0;
  background: url(../images/loader.gif) no-repeat 50% 40px;
}

/* LOGIN FORM, FORM ARTICLE */
.form-box-login,.form-article {
  background: #eee;
  margin: 10px 0 0 0;
  overflow: hidden;
  padding: 10px;
}

.form-article DIV {
  margin: 0 0 5px 0;
}

.form-article INPUT,.form-article TEXTAREA {
  width: 190px;
}

#user-name,#user-password,#simple-user-name,#simple-user-password {
  margin: 0 0 5px 0;
  width: 190px;
}

#simple-user-name,#simple-user-password {
  width: 167px;
}

.form-box-login .title {
  position: absolute;
  left: -9999px;
}

.form-box-login FIELDSET,.form-article FIELDSET {
  width: 213px;
  float: left;
  /*border-right: 1px solid #ccc;*/
  padding-right: 10px;
  margin-right: 10px;
}

.form-box-login LABEL
    .form-article LABEL {
  font-weight: normal;
  margin: 0 0 2px 0;
}

.form-box-login .radio-group,.form-article .radio-group {
  margin: 3px 0 10px;
}

.form-article .radio-group INPUT {
  width: auto;
}

.form-box-login .submit,.form-article .submit {
  margin: 0 5px 0 0;
}

.simple-login .radio-group,.simple-login .submit {
  margin: 0;
}
/* --------------------------------------------------------------
    Hides login activate link
    -------------------------------------------------------------- */
.login-activate {
  display: none;
}

.simple-login H3 {
  display: none;
}

.form-box-login .form-box-information,.form-article .form-box-information
  {
  width: 213px;
  float: left;
}

.form-box-login .form-box-information P,.form-article .form-box-information P
  {
  color: #666;
  line-height: 1.3;
}

.form-box-login .form-box-information .more {
  background: transparent url(../images/icons.gif) no-repeat scroll 0
    -357px;
  color: #0084FF;
  float: none;
  font-weight: bold;
  padding: 0 0 0 1.4em;
}

.logged-in H3 {
  position: absolute;
  left: -9999px;
}

.logged-in P {
  color: #333 !important;
  font-weight: bold;
  margin-bottom: 5px !important;
}

.logged-in LI {
  margin-top: 2px !important;
}

/* --------------------------------------------------------------
    Navbar top
    -------------------------------------------------------------- */
#navbar-top {
  position: fixed;
  top: 0;
  z-index: 10000;
  border-top: solid 1px #666;
  padding: 0 20px 0 20px;
  height: 27px;
  width: 960px;
  background: #333 url('../images/bg_navbar_top.png') repeat-x 0 0;
}

#navbar-top .content {
  font-size: 13px;
  line-height: 27px;
  color: white;
}

#navbar-top .content a {
  color: white;
}

#navbar-top .content ul {
  float: right;
  margin: 0 0 0 20px;
  padding: 0;
}

#navbar-top .content ul:first-child {
  float: none;
  margin-left: 0;
}

#navbar-top .content li {
  float: left;
  list-style: none;
  border-left: solid 1px #666;
  margin-left: 10px;
  padding-left: 9px;
}

#navbar-top .content li:first-child {
  border-left: 0;
  margin-left: 0;
  padding-left: 0;
}

