@font-face {
  font-family: "interstate-bold";
  src: url("fonts/Interstate-Black.otf") format("opentype");
  font-weight: normal;
  font-style: normal;
}
@font-face {
  font-family: "interstate-thin";
  src: url("fonts/Interstate-Light.otf") format("opentype");
  font-weight: normal;
  font-style: normal;
}
@font-face {
  font-family: "interstate-regular";
  src: url("fonts/Interstate-Regular.otf") format("opentype");
  font-weight: normal;
  font-style: normal;
}


/* FONTS & COLOURS */
html {
    background: #f2f6f9;
}
header {
    background: url(../images/mediawall/logo-dm.png) no-repeat 95% 80px;
}
#content p,
#ticker {
    color: #005488;
}
#ticker-footer {
	background-image: -webkit-gradient(linear, left top, right top, from(rgba(242, 246, 249, 0)), to(#f2f6f9));
}
#content h2 {
    font-family: "interstate-bold", arial, sans-serif;
}
#content p {
    font-family: "interstate-thin", arial, sans-serif;
}
#ticker {
    font-family: "interstate-regular", arial, sans-serif;
}
#ticker-header {
    font-family: "interstate-bold", arial, sans-serif;
}